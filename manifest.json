{
	"name": "Original Quality Images",
	"version": "0.3",
	"manifest_version": 2,
	"author": "Ivan Kozik",
	"description": "Redirects images on Twitter to original-quality images",
	"homepage_url": "https://github.com/ludios/original-quality-images",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"https://twitter.com/*",
		"https://pbs.twimg.com/media/*",
		"http://*.tumblr.com/*",
		"https://*.tumblr.com/*",
		"http://*.media.tumblr.com/*",
		"https://*.media.tumblr.com/*"
	]
}
